<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="Free HTML5 Website Template by GetTemplates.co" />
    <meta name="keywords" content="free website templates, free html5, free template, free bootstrap, free website template, html5, css3, mobile first, responsive" />
    <meta name="author" content="GetTemplates.co" />

    <!-- Facebook and Twitter integration -->
    <meta property="og:title" content="" />
    <meta property="og:image" content="" />
    <meta property="og:url" content="" />
    <meta property="og:site_name" content="" />
    <meta property="og:description" content="" />
    <meta name="twitter:title" content="" />
    <meta name="twitter:image" content="" />
    <meta name="twitter:url" content="" />
    <meta name="twitter:card" content="" />

    <link href="https://fonts.googleapis.com/css?family=Lato:300,400,700" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Kaushan+Script" rel="stylesheet">

    <!-- Animate.css -->
    <link rel="stylesheet" href="~/Content/Home/css/animate.css">
    <!-- Icomoon Icon Fonts-->
    <link rel="stylesheet" href="~/Content/Home/css/icomoon.css">
    <!-- Themify Icons-->
    <link rel="stylesheet" href="~/Content/Home/css/themify-icons.css">
    <!-- Bootstrap  -->
    <link rel="stylesheet" href="~/Content/Home/css/bootstrap.css">

    <!-- Magnific Popup -->
    <link rel="stylesheet" href="~/Content/Home/css/magnific-popup.css">

    <!-- Bootstrap DateTimePicker -->
    <link rel="stylesheet" href="~/Content/Home/css/bootstrap-datetimepicker.min.css">

    <!-- Owl Carousel  -->
    <link rel="stylesheet" href="~/Content/Home/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/Content/Home/css/owl.theme.default.min.css">

    <link href="~/Content/Admin/vendor/datatables/dataTables.bootstrap4.css" rel="stylesheet">

    <!-- Theme style  -->
    <link rel="stylesheet" href="~/Content/Home/css/style.css">
    <link rel="stylesheet" href="~/Content/Site.css">
    <!-- Modernizr JS -->
    <script src="~/Content/Home/js/modernizr-2.6.2.min.js"></script>
    <!-- FOR IE9 below -->
    <!--[if lt IE 9]>
    <script src="js/respond.min.js"></script>
    <![endif]-->

</head>
<body>

    <div class="gtco-loader"></div>

    <div id="page">


        <!-- <div class="page-inner"> -->
        <nav class="gtco-nav" role="navigation">
            <div class="gtco-container">

                <div class="row">
                    <div class="col-sm-4 col-xs-12">
                        <div id="gtco-logo"><a class="nav-link" href="@Url.Action("Index", "Home")">G-Hotel <em>.</em></a></div>
                    </div>
                    <div class="col-xs-8 text-right menu-1">
                        <ul>
                            <li class="has-dropdown">
                                <a href="#">Bảng giá</a>
                                <ul class="dropdown">
                                    <li><a class="nav-link" href="@Url.Action("GiaPhong", "BangGia")">Giá phòng</a></li>
                                    <li><a class="nav-link" href="@Url.Action("GiaDichVu", "BangGia")">Giá dịch vụ</a></li>
                                </ul>
                            </li>
                            <li><a class="nav-link" href="@Url.Action("About", "Home")">Giới thiệu</a></li>
                            <li><a class="nav-link" href="@Url.Action("Contact", "Home")">Liên hệ</a></li>
                            <li class="has-dropdown">
                                @{
                                    if (Session["KH"] != null)
                                    {
                                        QLKS.Models.tblKhachHang kh = (QLKS.Models.tblKhachHang)Session["KH"];

                                        <a href="#">@kh.ho_ten</a>
                                        <ul class="dropdown">
                                            <li><a href="@Url.Action("PhieuDatPhong", "Account")">Phiếu đặt phòng</a></li>
                                            <li><a href="@Url.Action("HoaDon", "Account")">Hóa Đơn</a></li>
                                            <li><a href="@Url.Action("CaNhan", "Account")">Sửa thông tin</a></li>
                                            <li><a onclick="return confirm('Bạn có thực sự muốn đăng xuất ?');" href="@Url.Action("Logout", "Account")">Logout</a></li>
                                        </ul>
                                    }
                                    else
                                    {
                                        <a href="#">Khách hàng</a>
                                        <ul class="dropdown">
                                            <li><a href="@Url.Action("Login", "Account")">Login</a></li>
                                            <li><a href="@Url.Action("Register", "Account")">Register</a></li>
                                        </ul>
                                    }
                                }
                            </li>
                            <li class="btn-cta"><a href="@Url.Action("BookRoom", "Home")"><span>Đặt phòng</span></a></li>
                        </ul>
                    </div>
                </div>

            </div>
        </nav>

        @RenderBody()

        <footer id="gtco-footer" role="contentinfo" style="background-image: url(/Content/Home/images/img_bg_1.jpg)" data-stellar-background-ratio="0.5">
            <div class="overlay"></div>
            <div class="gtco-container">
                <div class="row row-pb-md">




                    <div class="col-md-12 text-center">
                        <div class="gtco-widget">
                            <h3>Get In Touch</h3>
                            <ul class="gtco-quick-contact">
                                <li><a href="#"><i class="icon-phone"></i> +0123456789</a></li>
                                <li><a href="#"><i class="icon-mail2"></i> admin@ghotel.site</a></li>
                                <li><a href="#"><i class="icon-chat"></i> Live Chat</a></li>
                            </ul>
                        </div>
                        <div class="gtco-widget">
                            <h3>Get Social</h3>
                            <ul class="gtco-social-icons">
                                <li><a href="#"><i class="icon-twitter"></i></a></li>
                                <li><a href="#"><i class="icon-facebook"></i></a></li>
                                <li><a href="#"><i class="icon-linkedin"></i></a></li>
                                <li><a href="#"><i class="icon-dribbble"></i></a></li>
                            </ul>
                        </div>
                    </div>

                    <div class="col-md-12 text-center copyright">
                        <p>
                            <small class="block">&copy; 2023. All Rights Reserved.</small>
                            <small class="block">Designed and Developed by <a href="https://fb.com" target="_blank">Tung</a></small>
                        </p>
                    </div>

                </div>



            </div>
        </footer>
        <!-- </div> -->

    </div>

    <div class="gototop js-top">
        <a href="#" class="js-gotop"><i class="icon-arrow-up"></i></a>
    </div>

    <!-- jQuery -->
    <script src="~/Content/Home/js/jquery.min.js"></script>
    <!-- jQuery Easing -->
    <script src="~/Content/Home/js/jquery.easing.1.3.js"></script>
    <!-- Bootstrap -->
    <script src="~/Content/Home/js/bootstrap.min.js"></script>
    <!-- Waypoints -->
    <script src="~/Content/Home/js/jquery.waypoints.min.js"></script>
    <!-- Carousel -->
    <script src="~/Content/Home/js/owl.carousel.min.js"></script>
    <!-- countTo -->
    <script src="~/Content/Home/js/jquery.countTo.js"></script>

    <!-- Stellar Parallax -->
    <script src="~/Content/Home/js/jquery.stellar.min.js"></script>

    <!-- Magnific Popup -->
    <script src="~/Content/Home/js/jquery.magnific-popup.min.js"></script>
    <script src="~/Content/Home/js/magnific-popup-options.js"></script>

    <script src="~/Content/Home/js/moment.min.js"></script>
    <script src="~/Content/Home/js/bootstrap-datetimepicker.min.js"></script>


    <!-- Main -->
    <script src="~/Content/Home/js/main.js"></script>

</body>
</html>